    <script type="text/javascript">
        $(function () {
            replyLoad();
        });

        function replyLoad() {
            var reply_cookie = $.cookie(JmwInitJson.reply_list);
            if (reply_cookie != undefined && reply_cookie != "") {
                var reply_list = JSON.parse(reply_cookie);
                if (reply_list.length > 0) {
                    for (var i = 0; i < reply_list.length; i++) {
                        var reply_id = reply_list[i].reply_id;
                        var avatar = $.cookie('avatar');
                        var nick_name = $.cookie('nick_name');
                        var content = reply_list[i].content;
                        if (reply_id == "0") {
                            var hf_id = reply_list[i].id;
                            var is_after = true;//判断是否已有的回复
                            $(".jmw_hf").each(function () {
                                if (hf_id == $(this).attr("data-hf")) {
                                    is_after = false;
                                }
                            });
                            if (is_after) {
                                var html_hf = '<div class="mod_discuss clearfix jmw_hf" data-hf="' + hf_id + '"><a href="javascript:;" class="zan_left jmw_zan" data-hf="' + hf_id + '">0</a><div class="discmain">';
                                html_hf = html_hf + '<div class="uname"><img src="' + avatar + '" class="userpic"/><a>' + nick_name + '</a></div>';
                                html_hf = html_hf + '<div class="intro" id="imge0">' + content + '</div><div class="distool clearfix"><div class="disleft">';
                                html_hf = html_hf + '<a href="javascript:void(0);" class="taolun"><span style="font-size:14px;">讨论</span></a>刚刚</div><div class="disright"></div></div>';
                                html_hf = html_hf + '</div></div>';
                                
                                $(".jmw_hf:last").after(html_hf);
                            }
                        } else {
                            $(".jmw_hf").each(function () {
                                var tl_id = reply_list[i].id;
                                var hf_id = $(this).attr("data-hf");
                                if (hf_id == reply_id) {
                                    var is_after = true;//判断是否已有的讨论
                                    $(this).find(".jmw_tl").each(function () {
                                        if (tl_id == $(this).attr("data-tl")) {
                                            is_after = false;
                                        }
                                    });
                                    if (is_after) {
                                        var html_tl = '<div class="tbox jmw_tl" data-tl="' + tl_id + '"><div class="icon"></div><ul class="disclist"><li class="clearfix">';
                                        html_tl = html_tl + '<img src="' + avatar + '" class="imgbox"><div class="text"><h3><a class="">' + nick_name + '</a></h3>';
                                        html_tl = html_tl + '<div>' + content + '</div><div class="t">刚刚</div></div><div class="tmenu"></div></li></ul>';
                                        html_tl = html_tl + '<div class="addiscuss"></div></div>';
                                        
                                        if ($(this).find(".jmw_tl").length > 0) {
                                            $(this).find(".jmw_tl:last").after(html_tl);
                                        } else {
                                            $(this).find(".distool").after(html_tl);
                                        }
                                    }
                                }
                            });
                        }
                    }
                }
            }
        }
</script>
